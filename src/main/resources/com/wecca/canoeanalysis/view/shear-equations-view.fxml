   <children>
      <VBox alignment="CENTER" spacing="10" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0">
         <TableView fx:id="dataTable" prefHeight="450" prefWidth="750">
            <columns>
               <TableColumn fx:id="parameterColumn" prefWidth="150.0" text="Parameter"/>
               <TableColumn fx:id="descriptionColumn" prefWidth="250" text="Description"/>
               <TableColumn fx:id="valueColumn" prefWidth="350" text="Value"/>
