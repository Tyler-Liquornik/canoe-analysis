<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<AnchorPane
        prefHeight="600.0"
        prefWidth="850.0"
        stylesheets="/com/wecca/canoeanalysis/css/style.css"
        xmlns="http://javafx.com/javafx/15.0.1"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.wecca.canoeanalysis.controllers.popups.ShearEquationsController"
        style="-fx-background-color: #303030;">
   <children>
      <VBox alignment="CENTER"
            spacing="10"
            AnchorPane.leftAnchor="0"
            AnchorPane.rightAnchor="0"
            AnchorPane.topAnchor="0"
            AnchorPane.bottomAnchor="0">
         <TableView fx:id="dataTable"
                    prefHeight="450"
                    prefWidth="750">
            <columns>
               <TableColumn fx:id="parameterColumn" prefWidth="150.0" text="Parameter"/>
               <TableColumn fx:id="descriptionColumn" prefWidth="250" text="Description"/>
               <TableColumn fx:id="valueColumn" prefWidth="350" text="Value"/>
            </columns>
         </TableView>
      </VBox>
   </children>
</AnchorPane>




